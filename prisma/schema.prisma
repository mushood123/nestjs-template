generator client {
  provider     = "prisma-client"
  output       = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String      @id @default(cuid())
  email     String   @unique
  first_name      String 
  last_name       String
  password  String
  createdAt DateTime @default(now())
}
